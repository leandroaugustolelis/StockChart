ttomRight,\n  CompanySymbol,\n  CompanyName,\n  CompanyPrice,\n  MainContent,\n  Form,\n  Error,\n  Final,\n  PanelContent,\n  LiveButton,\n} from './styles';\nimport Title from '../../components/Title';\nimport TextInfo from '../../components/TextInfo';\nimport Input from '../../components/Input';\n\nimport { ReactComponent as StockImage } from '../../assets/stock-img.svg';\n\nimport StockChart from '../../components/StockChart';\nimport TickerPanel from '../../components/TickerPanel';\nimport LiveStockPanel from '../../components/LiveStockPanel';\n\nimport { StockData } from '../../store/ducks/stockdata/types';\n\nconst Main: React.FC = () => {\n  const [stockSymbol, setStockSymbol] = useState('');\n  const [stockInfo, setStockInfo] = useState<StockData>();\n  const [stockHistory, setStockHistory] = useState<any>();\n  const [loading, setLoading] = useState(false);\n  const setInputError = useDispatch();\n  const inputError = useSelector(\n    (state: ApplicationState) => state.stockdata.inputError,\n  );\n\n  const priceFormat = (item: number) => numeral(item).format('$ 0,0.00');\n\n  const handleSubmit = useCallback(\n    async (event: FormEvent<HTMLFormElement>): Promise<void> => {\n      event.preventDefault();\n\n      if (!stockSymbol) {\n        setInputError({\n          type: '@error/SET_ERROR',\n          payload: 'Please choose a stock',\n        });\n        return;\n      }\n\n      try {\n        const [dataInfo, dataHistory] = await Promise.all([\n          api.get(`/stable/stock/${stockSymbol}/quote`),\n          api.get(`/stable/stock/${stockSymbol}/chart/1y`),\n        ]);\n\n        setStockInfo(dataInfo.data);\n        setStockHistory(dataHistory.data);\n        setLoading(true);\n        setInputError({\n          type: '@error/SET_ERROR',\n          payload: null,\n        });\n      } catch (err) {\n        setInputError({\n          type: '@error/SET_ERROR',\n          payload: 'Stock does not exist',\n        });\n      }\n    },\n    [stockSymbol],\n  );\n\n  return (\n    <Container>\n      <Content>\n        <TickerPanel />\n        <TitleContent>\n          <Title text=\"historical chart\" />\n        </TitleContent>\n        <MainContent>\n          <ImageContent>\n            <StockImage />\n          </ImageContent>\n          <Form onSubmit={handleSubmit}>\n            <Input\n              placeholder=\"stock symbol\"\n              onChange={e => setStockSymbol(e.target.value)}\n              value={stockSymbol}\n            />\n            {inputError && <Error>{inputError}</Error>}\n          </Form>\n          <PanelContent>\n            <LiveStockPanel />\n          </PanelContent>\n        </MainContent>\n        <BottomContent>\n          <BottomLeft>\n            {loading && <TextInfo text=\"symbol\"></TextInfo>}\n            {loading && <TextInfo text=\"company\"></TextInfo>}\n            {loading && <TextInfo text=\"price\"></TextInfo>}\n          </BottomLeft>\n          <BottomRight>\n            <CompanySymbol>{loading && stockInfo?.symbol}</CompanySymbol>\n            <CompanyName>{loading && stockInfo?.companyName}</CompanyName>\n            <CompanyPrice>\n              {loading && stockInfo && `${priceFormat(stockInfo.latestPrice)}`}\n            </CompanyPrice>\n            <LiveButton>\n              {loading && (\n                <button>\n                  <AiOutlinePlusSquare />\n                </button>\n              )}\n              {loading && <span>Real Time Panel</span>}\n            </LiveButton>\n          </BottomRight>\n        </BottomContent>\n        <Final>{loading && <StockChart data={stockHistory} />}</Final>\n      </Content>\n    </Container>\n  );\n};\n\nexport default Main;\n","/home/leandroaugustolelis/Projects/stockchart/src/pages/Main/styles.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/TextInfo/index.tsx",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/Title/index.tsx",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/Title/styles.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/TextInfo/styles.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/Input/index.tsx",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/Input/styles.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/StockChart/index.tsx",["114"],"import React from 'react';\n\nimport { AreaChart, Area, XAxis, YAxis, Tooltip } from 'recharts';\n\nimport CustomTooltip from '../CustomTooltip/index';\n\nconst StockChart: React.FC<any> = ({ data }) => {\n  return (\n    <AreaChart\n      width={500}\n      height={150}\n      data={data}\n      margin={{\n        top: 30,\n        right: 0,\n        left: 0,\n        bottom: 0,\n      }}\n    >\n      <defs>\n        <linearGradient id=\"colorValue\" x1=\"0\" y1=\"0\" x2=\"0\" y2=\"1\">\n          <stop offset=\"45%\" stopColor=\"#f7931a\" stopOpacity={1} />\n          <stop offset=\"100%\" stopColor=\"#f7931a\" stopOpacity={0.0} />\n        </linearGradient>\n      </defs>\n      <XAxis tick={false} axisLine={false} />\n      <YAxis tick={false} axisLine={false} />\n\n      <Area\n        type=\"linear\"\n        dataKey=\"close\"\n        stroke=\"none\"\n        fill=\"url(#colorValue)\"\n        fillOpacity={1}\n      />\n      <Tooltip isAnimationActive={false} content={<CustomTooltip />} />\n    </AreaChart>\n  );\n};\n\nexport default StockChart;\n","/home/leandroaugustolelis/Projects/stockchart/src/services/api.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/store/index.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/CustomTooltip/index.tsx",["115","116"],"import React from 'react';\nimport numeral from 'numeral';\n\nimport Container from './styles';\n\ninterface Props {\n  active?: boolean;\n  payload?: Array<any>;\n  label?: string;\n  customProps?: string;\n  customProps1?: string;\n  customProps2?: string;\n}\n\nconst CustomTooltip = ({ active, payload }: Props) => {\n  const priceFormat = (item: string) => numeral(item).format('$ 0,0.00');\n  const volumeFormat = (item: string) => numeral(item).format('$ 0,0');\n\n  if (active && payload) {\n    return (\n      <Container>\n        <p>{`Price : ${priceFormat(payload[0].payload.close)}`}</p>\n        <p>{`Date : ${payload[0].payload.label}`}</p>\n        <p>{`Volume : ${volumeFormat(payload[0].payload.volume)}`}</p>\n      </Container>\n    );\n  }\n\n  return null;\n};\n\nexport default CustomTooltip;\n","/home/leandroaugustolelis/Projects/stockchart/src/components/CustomTooltip/styles.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/LiveStockPanel/index.tsx",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/LiveStockPanel/styles.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/store/ducks/rootReducer.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/store/ducks/stockdata/index.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/store/ducks/stockdata/types.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/Error/index.tsx",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/Error/styles.ts",[],["117","118","119","120","121"],"/home/leandroaugustolelis/Projects/stockchart/src/components/TickerPanel/index.tsx",["122","123","124"],"import React, { useEffect } from 'react';\nimport { useDispatch, connect } from 'react-redux';\n\nimport Ticker from 'react-ticker';\nimport api from '../../services/api';\n\nimport { Container, CompanyName, Variation, StockPrice } from './styles';\n\nimport { ApplicationState } from '../../store';\nimport { StockData } from '../../store/ducks/stockdata/types';\n\ninterface TickerProps {\n  dataTicker1: any | StockData;\n  dataTicker2: any | StockData;\n  dataTicker3: any | StockData;\n}\n\nconst TickerPanel = ({\n  dataTicker1,\n  dataTicker2,\n  dataTicker3,\n}: TickerProps) => {\n  const loadDataTicker = useDispatch();\n\n  useEffect