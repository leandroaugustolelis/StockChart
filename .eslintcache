rom '../../components/TextInfo';\nimport Input from '../../components/Input';\n\nimport { ReactComponent as StockImage } from '../../assets/stock-img.svg';\n\nimport StockChart from '../../components/StockChart';\n\ninterface StockProps {\n  symbol: string;\n  companyName: string;\n  latestPrice: number;\n}\n\ninterface ReduxProps {\n  loadedStockInfo: string;\n}\n\nconst Main: React.FC = () => {\n  const [stockSymbol, setStockSymbol] = useState('');\n  const [stockInfo, setStockInfo] = useState<StockProps>();\n  const [stockHistory, setStockHistory] = useState();\n  const [loading, setLoading] = useState(false);\n  const testRedux = useSelector<ReduxProps>(state => state);\n\n  console.log(testRedux);\n\n  async function handleSubmit(\n    event: FormEvent<HTMLFormElement>,\n  ): Promise<void> {\n    event.preventDefault();\n\n    const [dataInfo, dataHistory] = await Promise.all([\n      api.get(`stable/stock/${stockSymbol}/quote`),\n      api.get(`/stable/stock/${stockSymbol}/chart/1y`),\n    ]);\n\n    setStockInfo(dataInfo.data);\n    setStockHistory(dataHistory.data);\n    setLoading(true);\n  }\n\n  return (\n    <Container>\n      <Content>\n        <ContentTop>\n          <ImageContent>\n            <StockImage />\n          </ImageContent>\n          <Test>\n            <TitleContent>\n              <Title text=\"historical chart\" />\n            </TitleContent>\n            <Form onSubmit={handleSubmit}>\n              <Input\n                placeholder=\"type a symbol\"\n                onChange={e => setStockSymbol(e.target.value)}\n                isFocus={false}\n                isError={true}\n                value={stockSymbol}\n              />\n            </Form>\n          </Test>\n        </ContentTop>\n        <BottomContent>\n          <BottomLeft>\n            {loading && <TextInfo text=\"symbol\"></TextInfo>}\n            {loading && <TextInfo text=\"company\"></TextInfo>}\n            {loading && <TextInfo text=\"price\"></TextInfo>}\n          </BottomLeft>\n          <BottomRight>\n            <CompanySymbol>{loading && stockInfo?.symbol}</CompanySymbol>\n            <CompanyName>{loading && stockInfo?.companyName}</CompanyName>\n            <CompanyPrice>\n              {loading && `$ ${stockInfo?.latestPrice}`}\n            </CompanyPrice>\n          </BottomRight>\n        </BottomContent>\n        <Final>{loading && <StockChart data={stockHistory} />}</Final>\n      </Content>\n    </Container>\n  );\n};\n\nexport default Main;\n","/home/leandroaugustolelis/Projects/stockchart/src/pages/Main/styles.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/TextInfo/index.tsx",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/Title/index.tsx",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/Title/styles.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/TextInfo/styles.ts",[],["85","86","87","88","89"],"/home/leandroaugustolelis/Projects/stockchart/src/components/Input/index.tsx",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/Input/styles.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/StockChart/index.tsx",["90"],"import React from 'react';\nimport moment from 'moment';\nimport numeral from 'numeral';\n\nimport { AreaChart, Area, XAxis, YAxis, Tooltip } from 'recharts';\n\nimport ResponsiveContainer from './styles';\n\nconst styles = {\n  tooltipWrapper: {\n    background: '#444444',\n    border: 'none',\n  },\n  tooltip: {\n    color: '#ebebeb',\n  },\n};\n\nconst StockChart: React.FC<any> = ({ data }) => {\n  const currencyFormat = (item: string) => numeral(item).format('$0,0');\n  const dateFormat = (item: Date) => moment(item).format('MMM YY');\n\n  return (\n    <ResponsiveContainer>\n      <AreaChart\n        width={600}\n        height={250}\n        data={data}\n        margin={{\n          top: 30,\n          right: 0,\n          left: 0,\n          bottom: 0,\n        }}\n      >\n        <defs>\n          <linearGradient id=\"colorValue\" x1=\"0\" y1=\"0\" x2=\"0\" y2=\"1\">\n            <stop offset=\"45%\" stopColor=\"#f7931a\" stopOpacity={1} />\n            <stop offset=\"100%\" stopColor=\"#f7931a\" stopOpacity={0.0} />\n          </linearGradient>\n        </defs>\n        <XAxis\n          tick={false}\n          axisLine={false}\n          dataKey=\"close\"\n          tickFormatter={dateFormat}\n        />\n        <YAxis tick={false} axisLine={false} tickFormatter={currencyFormat} />\n        <Tooltip\n          itemStyle={{ fontSize: 20 }}\n          contentStyle={styles.tooltipWrapper}\n          labelStyle={styles.tooltip}\n        />\n        <Area\n          type=\"linear\"\n          dataKey=\"close\"\n          name=\"price\"\n          stroke=\"none\"\n          fill=\"url(#colorValue)\"\n          fillOpacity={1}\n        />\n      </AreaChart>\n    </ResponsiveContainer>\n  );\n};\n\nexport default StockChart;\n","/home/leandroaugustolelis/Projects/stockchart/src/components/StockChart/styles.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/services/api.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/store/index.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/store/createStore.ts",["91"],"import { applyMiddleware, createStore, Middleware, Reducer } from 'redux';\n\nimport { DataAction, DataState } from './modules/data/types';\n\nexport interface StoreState {\n  data: DataState;\n}\n\nexport type StoreAction = DataAction;\n\nexport default (\n  reducers: Reducer<StoreState, StoreAction>,\n  middlewares: Middleware[],\n) => {\n  const enhancer = applyMiddleware(...middlewares);\n\n  return createStore(reducers, enhancer);\n};\n","/home/leandroaugustolelis/Projects/stockchart/src/store/modules/rootReducer.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/store/modules/data/reducer.ts",[],{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","replacedBy":"97"},{"ruleI