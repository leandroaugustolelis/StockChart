   <Test>\n            <TitleContent>\n              <Title text=\"historical chart\" />\n            </TitleContent>\n            <Form onSubmit={handleSubmit}>\n              <Input\n                placeholder=\"type a symbol\"\n                onChange={e => setStockSymbol(e.target.value)}\n                isFocus={false}\n                isError={true}\n                value={stockSymbol}\n              />\n            </Form>\n          </Test>\n        </ContentTop>\n        <BottomContent>\n          <BottomLeft>\n            {loading && <TextInfo text=\"symbol\"></TextInfo>}\n            {loading && <TextInfo text=\"company\"></TextInfo>}\n            {loading && <TextInfo text=\"price\"></TextInfo>}\n          </BottomLeft>\n          <BottomRight>\n            <CompanySymbol>{loading && stockInfo?.symbol}</CompanySymbol>\n            <CompanyName>{loading && stockInfo?.companyName}</CompanyName>\n            <CompanyPrice>\n              {loading && `$ ${stockInfo?.latestPrice}`}\n            </CompanyPrice>\n          </BottomRight>\n        </BottomContent>\n        <Final>{loading && <StockChart data={stockHistory} />}</Final>\n      </Content>\n    </Container>\n  );\n};\n\nexport default Main;\n","/home/leandroaugustolelis/Projects/stockchart/src/pages/Main/styles.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/TextInfo/index.tsx",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/Title/index.tsx",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/Title/styles.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/TextInfo/styles.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/Input/index.tsx",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/Input/styles.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/StockChart/index.tsx",["95","96"],"/home/leandroaugustolelis/Projects/stockchart/src/components/StockChart/styles.ts",[],"/home/lean