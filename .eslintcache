yName,\n  CompanyPrice,\n  MainContent,\n  Form,\n  Error,\n  Final,\n  PanelContent,\n  LiveButton,\n} from './styles';\nimport Title from '../../components/Title';\nimport TextInfo from '../../components/TextInfo';\nimport Input from '../../components/Input';\n\nimport { ReactComponent as StockImage } from '../../assets/stock-img.svg';\n\nimport StockChart from '../../components/StockChart';\nimport Ticker from '../../components/Ticker';\nimport LiveStockPanel from '../../components/LiveStockPanel';\n\nimport { StockData } from '../../store/ducks/stockdata/types';\n\nconst Main: React.FC = () => {\n  const [stockSymbol, setStockSymbol] = useState('');\n  const [stockInfo, setStockInfo] = useState<StockData>();\n  const [stockHistory, setStockHistory] = useState<any>();\n  const [loading, setLoading] = useState(false);\n  const setInputError = useDispatch();\n  const inputError = useSelector(\n    (state: ApplicationState) => state.stockdata.inputError,\n  );\n\n  const handleSubmit = useCallback(\n    async (event: FormEvent<HTMLFormElement>): Promise<void> => {\n      event.preventDefault();\n\n      if (!stockSymbol) {\n        setInputError({\n          type: '@error/SET_ERROR',\n          payload: 'Please choose a stock',\n        });\n        return;\n      }\n\n      try {\n        const [dataInfo, dataHistory] = await Promise.all([\n          api.get(`/stable/stock/${stockSymbol}/quote`),\n          api.get(`/stable/stock/${stockSymbol}/chart/1y`),\n        ]);\n\n        setStockInfo(dataInfo.data);\n        setStockHistory(dataHistory.data);\n        setLoading(true);\n      } catch (err) {\n        setInputError({\n          type: '@error/SET_ERROR',\n          payload: 'Stock does not exist',\n        });\n      }\n    },\n    [stockSymbol],\n  );\n\n  const addToPanel = () => {\n    console.log('lalala');\n  };\n\n  return (\n    <Container>\n      <Content>\n        <Ticker />\n        <TitleContent>\n          <Title text=\"historical chart\" />\n        </TitleContent>\n        <MainContent>\n          <ImageContent>\n            <StockImage />\n          </ImageContent>\n          <Form onSubmit={handleSubmit}>\n            <Input\n              placeholder=\"stock symbol\"\n              onChange={e => setStockSymbol(e.target.value)}\n              value={stockSymbol}\n            />\n            {inputError && <Error>{inputError}</Error>}\n          </Form>\n\n          <PanelContent>\n            <LiveStockPanel />\n          </PanelContent>\n        </MainContent>\n\n        <BottomContent>\n          <BottomLeft>\n            {loading && <TextInfo text=\"symbol\"></TextInfo>}\n            {loading && <TextInfo text=\"company\"></TextInfo>}\n            {loading && <TextInfo text=\"price\"></TextInfo>}\n          </BottomLeft>\n          <BottomRight>\n            <CompanySymbol>{loading && stockInfo?.symbol}</CompanySymbol>\n            <CompanyName>{loading && stockInfo?.companyName}</CompanyName>\n            <CompanyPrice>\n              {loading && `$ ${stockInfo?.latestPrice}`}\n            </CompanyPrice>\n            <LiveButton>\n              {loading && (\n                <button onClick={addToPanel}>\n                  <AiOutlinePlusSquare />\n                </button>\n              )}\n              {loading && <span>Real Time Panel</span>}\n            </LiveButton>\n          </BottomRight>\n        </BottomContent>\n        <Final>{loading && <StockChart data={stockHistory} />}</Final>\n      </Content>\n    </Container>\n  );\n};\n\nexport default Main;\n","/home/leandroaugustolelis/Projects/stockchart/src/pages/Main/styles.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/TextInfo/index.tsx",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/Title/index.tsx",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/Title/styles.ts",[],"/home/leandroaugustolelis/Projects/stockchart/src/components/TextInfo/styles.ts",[],"/home/leandroaugustolelis/Pr